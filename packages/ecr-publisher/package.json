{
  "name": "@98kb/ecr-publisher",
  "version": "1.0.2",
  "description": "A CLI tool to automatically version, build, and publish Docker images to AWS ECR (Elastic Container Registry).",
  "author": "dsfx3d",
  "repository": {
    "type": "git",
    "url": "https://github.com/98kb/aws.git"
  },
  "homepage": "https://github.com/98kb/aws/tree/main/packages/ecr-publisher#98kbecr-publisher",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ecr-publisher": "dist/index.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build:tsup": "tsup src/index.ts",
    "typecheck": "tsc --noEmit",
    "dev": "pnpx tsx src/index.ts",
    "build": "pnpm build:tsup && pnpm typecheck"
  },
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "@aws-sdk/client-ecr": "^3.835.0",
    "commander": "^14.0.0",
    "dockernode": "^0.1.0",
    "fp-ts": "^2.16.10",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/pg": "^8.15.2",
    "@types/semver": "^7.7.0",
    "dotenv": "^16.5.0",
    "pg": "^8.16.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}